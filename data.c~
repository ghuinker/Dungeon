#include <stdlib.h>
#include <stdio.h>
#include <string.h>

#include "data.h"

int readIn(int argc, char*argv[]){
  
  if (argc != 5 || argc != 11) {
    fprintf(stderr, "Bad arguments.\n");

    return -1;
  }

  /* Valid switches are -load or -save */
  if(argc >=5)
    if(argv[1][0] != '-'){
      fprintf(stderr, "Bad argument format.\n");

    return -1;
    }

  if(argc ==11)
    if (argv[7][0] != '-') {
      fprintf(stderr, "Bad argument format.\n");

      return -1;
    }

  printf("%s", *argv);


  
  return 1;
}

